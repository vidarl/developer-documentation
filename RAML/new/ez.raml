                    description: The RoleInput schema encoded in XML or JSON.
                    example:  |
                        application/vnd.ez.api.RoleInput+json
                        application/vnd.ez.api.RoleInput+xml
            body:
                 application/vnd.ez.api.RoleInput+xml:
                    type: RoleInput
